{
    "name": "roomies/fraudable",
    "description": "Integrate your Laravel app with AWS Fraud Detector",
    "keywords": [
        "roomies",
        "laravel",
        "aws fraud detector"
    ],
    "homepage": "https://github.com/roomies-com/fraudable",
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "hello@roomies.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.320",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Roomies\\Fraudable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roomies\\Fraudable\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/pint"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roomies\\Fraudable\\FraudableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
